<!DOCTYPE html>
<html>
<head>
  <title>{{ api_name }}</title>
  <meta charset="utf-8">
  <link rel="stylesheet" type="text/css" href="{{bootstrap_css_url}}" />
</head>
<body>
<div class="container documentation">
  <h1>{{ api_name }}</h1>

  <table class="table table-striped table-bordered">
    <tbody>
      {{# sections }}
      <tr>
        <th colspan="2">{{ resource_name }}</th>
      </tr>
      {{# examples }}
      <tr>
        <th>&nbsp;</th>
        <td><a href="{{ dirname }}/{{ filename }}">{{ description }}</a></td>
      </tr>
      {{/ examples }}
      {{/ sections }}
    </tbody>
  </table>
</div>
</body>
</html>
