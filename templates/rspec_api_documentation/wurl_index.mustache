<!DOCTYPE html>
<html>
<head>
  <title>{{ api_name }}</title>
  <link rel="stylesheet" href="{{ url_prefix }}/assets/stylesheets/bootstrap.css"/>
</head>
<body>
<div class="container">
  <h1>{{ api_name }}</h1>

  {{# sections }}
  <div class="article">
    <h2>{{ resource_name }}</h2>

    <ul>
      {{# examples }}
      <li>
        <a href="{{ url_prefix }}/{{ dirname }}/{{ filename }}">{{ description }}</a>
      </li>
      {{/ examples }}
    </ul>
  </div>
  {{/ sections }}
</div>
</body>
</html>
